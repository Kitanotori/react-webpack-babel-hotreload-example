{
	"eslintignore": ["node_modules"],
	"env": {
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended"],
	"rules": {
		"no-console": 0,
		"indent": ["error", "tab"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"curly": ["error", "multi-line"],
		"no-unused-vars": "warn"
	}
}
